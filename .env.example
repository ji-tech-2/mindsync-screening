# Environment variables for Inference Service

# Weights & Biases Configuration (REQUIRED for auto-download)
WANDB_API_KEY=your_wandb_api_key_here
WANDB_PROJECT=mindsync-model
WANDB_ENTITY=your_wandb_username_or_team
ARTIFACT_VERSION=latest

# Skip W&B download if you have artifacts locally
SKIP_WANDB_DOWNLOAD=false

# Google Gemini AI (REQUIRED for advice generation)
GEMINI_API_KEY=your_gemini_api_key_here

# Database Configuration
DATABASE_URL=postgresql://mindsync:mindsync_dev_password@localhost:5432/mindsync

# Redis/Valkey Cache Configuration
VALKEY_URL=redis://localhost:6379

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=1
